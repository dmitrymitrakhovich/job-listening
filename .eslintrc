{
  "env": {
    "es6": true
  },
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "prefer-const": "error",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-single"],
    "no-plusplus": "error",
    "max-len": ["error", 120, { "ignoreUrls": true }],
    "comma-dangle": ["error", "always-multiline"],
    "no-unused-vars": "error"
  }
}
